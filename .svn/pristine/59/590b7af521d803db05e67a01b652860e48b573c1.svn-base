                            " FROM I_PaymentUpdateSub up ,OT_InstanceContext ic ,I_PaymentSub p " +
                            " where p.ObjectID ='"+ dr["PaymentId"].ToString() + "' " +
                            " and up.ObjectID = ic.BizObjectId and p.paymentNo = up.paymentno " +
