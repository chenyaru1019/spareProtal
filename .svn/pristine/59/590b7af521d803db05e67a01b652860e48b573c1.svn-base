                                dic2.Add("WorkItemId", workItemId2);
                                dic2.Add("PaymentId", dr2["PaymentId"].ToString());
                                dic2.Add("changePaymentFlg", dr2["changePaymentFlg"].ToString());
                                dic2.Add("RejectFlg", dr2["RejectFlg"].ToString());
                                ls.Add(dic2);
