                        bizObjects[j]["PaymentId"] = dic["PaymentId"].ToString();
                        bizObjects[j]["changePaymentFlg"] = dic["changePaymentFlg"].ToString();
                        bizObjects[j]["RejectFlg"] = dic["RejectFlg"].ToString();
                        j++;
