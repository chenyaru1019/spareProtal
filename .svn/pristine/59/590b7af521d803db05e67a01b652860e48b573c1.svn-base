                        System.Data.DataTable dtu = OThinker.H3.Controllers.AppUtility.Engine.EngineConfig.CommandFactory.CreateCommand().ExecuteDataTable(
                            " SELECT p.BankName," +
                            " up.PaymentAmount," +
                            " ic.State Status," +
                            " case ic.State " +
                            " when '4' then '已开证' " +
