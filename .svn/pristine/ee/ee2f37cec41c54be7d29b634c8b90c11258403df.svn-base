function upmoney() {
    //循环获取本次收款
    var sum = 0;
    $('table#Control18 input[data-datafield="Charge_pro.this_receipt"]').each(function (i, n) {
       
        sum += parseInt($(n).val());
    });
    $.MvcSheetUI.SetControlValue("Reduced_money", sum);//折算金额
    $.MvcSheetUI.SetControlValue("Collection_money", sum);//收款
}